{
	"name": "typescript-project-template",
	"version": "1.0.0",
	"description": "TypeScript Node.js project template",
	"engines": {
		"node": ">= 14.x",
		"npm": ">= 6.x"
	},
	"type": "module",
	"main": "./dist/index.js",
	"dependencies": {},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "4.x",
		"ava": "3.x",
		"eslint": "7.x",
		"eslint-config-standard-with-typescript": "19.x",
		"eslint-plugin-import": "2.x",
		"eslint-plugin-node": "11.x",
		"eslint-plugin-promise": "4.x",
		"eslint-plugin-standard": "4.x",
		"ts-node": "9.x",
		"typescript": "4.x"
	},
	"scripts": {
		"lint": "eslint *.cjs . || cd .",
		"fix": "eslint --fix *.cjs . || cd .",
		"build": "tsc",
		"test": "ava",
		"prepack": "npm run build"
	},
	"license": "UNLICENSED",
	"private": true
}
